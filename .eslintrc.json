{
  "env": {
    "es6": true,
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base","prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": true}],
    "consistent-return": "off" 
  }
}
